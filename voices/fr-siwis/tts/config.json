{
    "model": "glow_tts",
    "run_name": "siwis",
    "run_description": "glow-tts model training with gated conv.",
    "audio": {
        "fft_size": 1024,
        "win_length": 1024,
        "hop_length": 256,
        "frame_length_ms": null,
        "frame_shift_ms": null,
        "sample_rate": 22050,
        "preemphasis": 0.0,
        "ref_level_db": 0,
        "power": 1.1,
